{
  "mcpServers": {
    "dot-ai": {
      "command": "docker",
      "args": [
        "compose",
        "-f",
        "docker-compose-dot-ai.yaml",
        "run",
        "--rm",
        "--remove-orphans",
        "dot-ai"
      ],
      "env": {
        "AI_PROVIDER": "anthropic_haiku"
      }
    }
  }
}